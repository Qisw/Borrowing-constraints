\begin_deeper
\begin_layout Standard
State vector:
\end_layout

\begin_layout Itemize
assets (or debts) 
\begin_inset Formula $k$
\end_inset


\end_layout

\begin_layout Itemize
ability 
\begin_inset Formula $\ability$
\end_inset


\end_layout

\begin_layout Itemize
schooling 
\begin_inset Formula $s\in\left\{ HSG,CD,CG\right\} $
\end_inset


\end_layout

\begin_layout Standard
Household problem:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
V_{w}\left(k,\ability,s\right)=\max_{c_{\age}}\sum_{\age=1}^{\ageMax-\tSchool}\beta^{t-1}u_{w}\left(c_{\age}\right)
\end{equation}

\end_inset

subject to a lifetime budget constraint
\begin_inset Formula 
\begin{equation}
\sum_{\age=1}^{\ageMax-\tSchool}R^{1-\age}c_{\age}=\pvEarn\left(s,\ability\right)+Rk
\end{equation}

\end_inset


\end_layout

\end_deeper
