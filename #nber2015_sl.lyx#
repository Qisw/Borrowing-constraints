\begin_deeper
\begin_layout Standard
Each family is endowed with a type 
\begin_inset Formula $j\in\left\{ 1,...,J\right\} $
\end_inset


\end_layout

\begin_layout Standard
All agents of type 
\begin_inset Formula $j$
\end_inset

 share the same values for
\end_layout

\begin_layout Itemize
parental income 
\begin_inset Formula $\famIncome$
\end_inset


\end_layout

\begin_layout Itemize
college cost 
\begin_inset Formula $\collCost$
\end_inset


\end_layout

\begin_layout Itemize
ability signal 
\begin_inset Formula $\abilSignal$
\end_inset


\end_layout

\begin_layout Itemize
preference for college (details below)
\end_layout

\begin_layout Standard
Ability 
\begin_inset Formula $\ability$
\end_inset

 is not observed until the start of work.
\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
College Entry Decision
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\max\{\underset{\mbox{work as HSG}}{\underbrace{V_{HS}\left(j\right)-\prefScaleEntry\prefShockEntry_{w}}},\underset{\mbox{enter college}}{\underbrace{V_{entry}\left(j\right)-\prefScaleEntry\prefShockEntry_{c}}}\}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\prefShockEntry_{c},\prefShockEntry_{w}$
\end_inset

: type I extreme value shocks (for computational reasons)
\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Working as HSG
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
Value of working as HSG:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
V_{HS}\left(j\right)=\max_{z_{w}\ge0}u_{p}\left(\famIncome-z_{w}\right)+\mathbb{E}_{a}\left\{ V_{w}\left(z_{w},HS,\ability\right)|j\right\} +\prefHS
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\famIncome$
\end_inset

: parental income 
\end_layout

\begin_layout Standard
\begin_inset Formula $z$
\end_inset

: transfer to the child 
\end_layout

\begin_layout Standard
\begin_inset VSpace defskip
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\prefHS$
\end_inset

: common preference for working as HSG
\end_layout

\begin_layout Itemize
permits the model to match overall college entry rate for each cohort
\begin_inset VSpace defskip
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Note
status open

\begin_layout Plain Layout
add: hetero in parental altruism
\end_layout

\end_inset


\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Work Phase
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\begin{equation}
V_{w}\left(k,\ability,s\right)=\max_{c_{\age}}\sum_{\age=1}^{\ageMax-\tSchool}\beta^{t-1}u_{w}\left(c_{\age}\right)
\end{equation}

\end_inset

subject to a lifetime budget constraint
\begin_inset Formula 
\begin{equation}
\sum_{\age=1}^{\ageMax-\tSchool}R^{1-\age}c_{\age}=\pvEarn\left(s,\ability\right)+Rk
\end{equation}

\end_inset


\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Value of College Entry
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\begin{equation}
V_{entry}\left(j\right)=\max_{z_{c}\ge0}u_{p}\left(\famIncome-z_{c}\right)+V_{1}\left(z_{c},j\right)
\end{equation}

\end_inset


\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Years 1-2 In College
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\begin{equation}
V_{1}\left(k,j\right)=\max_{k',c,l}\left(1+\beta\right)u\left(c+\cColl_{j},1+\lColl_{j}-l\right)+\beta^{2}V_{m}\left(k',j\right)
\end{equation}

\end_inset

subject to 
\end_layout

\begin_layout Itemize
budget constraint: 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $k'=Rk+2\left(\collegeWage l-\collCost_{j}-c\right)$
\end_inset


\end_layout

\begin_layout Itemize
borrowing constraint: 
\begin_inset Formula $k'\ge k_{min,3}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\cColl_{j},\lColl_{j}$
\end_inset

: increasing in 
\begin_inset Formula $\abilSignal$
\end_inset


\end_layout

\begin_layout Itemize
prevents high ability students from consuming too much in college
\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
End of Year 2 in College
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
With probability 
\begin_inset Formula $1-\pi\left(\ability\right)$
\end_inset

: drop out and start working.
 
\end_layout

\begin_layout Standard
Otherwise: remain in college for 2 more years.
\end_layout

\begin_layout Standard
\begin_inset VSpace bigskip
\end_inset


\end_layout

\begin_layout Standard
Continuation value:
\begin_inset Formula 
\begin{equation}
V_{m}\left(k,j\right)=\mathbb{E}_{\ability}\left[\left(1-\pi\left[\ability\right]\right)V_{w}\left(k,\ability,CD\right)+\pi\left[\ability\right]V_{3}\left(k,j\right)\right]
\end{equation}

\end_inset


\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Years 3-4 In College
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
V_{3}\left(k,j\right)=\max_{k',c,l}\left(1+\beta\right)u(c+\cColl_{j},1+\lColl_{j}-l)+\beta^{2}\mathbb{E}_{\ability}V_{w}\left(k',\ability,CG\right)
\]

\end_inset


\end_layout

\begin_layout Standard
subject to 
\end_layout

\begin_layout Itemize
budget constraint
\end_layout

\begin_layout Itemize
borrowing constraint
